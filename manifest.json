{
  "manifest_version": 2,

  "name": "Video Playback Speed",
  "description": "This extension allows the user to change the video playback speed.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
      "scripts": ["popup.js"],
      "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
